{
  "apiVersion": "extensions/v1beta1",
  "kind": "Deployment",
  "metadata": {
    "name": "mongodb"
  },
  "spec": {
    "replicas": 1,
    "template": {
      "metadata": {
        "labels": {
          "mongodb": "true"
        }
      },
      "spec": {
        "containers": [
          {
            "image": "mongo:3.6.11-stretch",
            "name": "my-mongodb",
            "resources": {
              "requests": { "cpu": "0m", "memory": "100Mi" }
            }
          }
        ]
      }
    }
  }
}
