{
  "name": "discourse",
  "containers": [
    {
      "env": [
        {
          "name": "POSTGRES_USER",
          "value": "root"
        }
      ],
      "image": "postgres:10.5",
      "name": "my-postgres"
    },
    {
      "name": "target",
      "image": "gcr.io/athena-fuzzer/discourse:8f1bc6525d",
      "command": [
        "./run_server.sh"
      ],
      "env": [
        {
          "name": "RESULTS_PATH",
          "value": "/tmp/results"
        }
      ],
      "volumeMounts": [
        {
          "mountPath": "/tmp/results",
          "name": "results-dir"
        }
      ]
    }
  ]
}
