{
  "port": 8080,
  "db": {
    "name": "fuzz_db",
    "port": 5432,
    "host": "localhost",
    "user": "root",
    "password": "mysecretpassword"
  },
  "name": "discourse",
  "containers": [
    {
      "env": [
        {
          "name": "POSTGRES_USER",
          "value": "root"
        }
      ],
      "image": "postgres:10.5",
      "name": "my-postgres"
    },
    {
      "name": "target",
      "image": "gcr.io/athena-fuzzer/discourse:8f1bc6525d",
      "command": [
        "./run_server.sh"
      ]
    }
  ]
}
